{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "@roninbuilders/modal#build":{
      "outputs": ["dist/**"],
      "dependsOn": ["^@roninbuilders/modal-ui#build"],
      "cache": false
    },
    "@roninbuilders/modal-wagmi#build":{
      "outputs": ["dist/**"],
      "dependsOn": ["^@roninbuilders/modal-ui#build"],
      "cache": false
    },
    "react-wagmi":{
      "outputs": ["dist/**"],
      "dependsOn": ["^@roninbuilders/modal-wagmi#build"],
      "cache": false
    },
    "react-w3vm":{
      "outputs": ["dist/**"],
      "dependsOn": ["^@roninbuilders/modal#build"],
      "cache": false
    },
    "html-w3vm":{
      "outputs": ["dist/**"],
      "dependsOn": ["^@roninbuilders/modal#build"],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "watch":{
      "cache": false
    }
  }
}
